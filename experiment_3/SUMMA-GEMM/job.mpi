#!/bin/bash

#SBATCH -J summa2_a_8k         # Job name
#SBATCH -o summa2_a_8k.%j.out         # Name of stdout output file (%j expands to jobId)
#SBATCH -N 1                  # Total number of nodes requested
#SBATCH -n 4                 # Total number of mpi tasks requested
#SBATCH -p rome
#SBATCH -t 08:00:00           # Run time (hh:mm:ss) - 1.5 hours

# Launch MPI-based executable

mpirun -np 4 ./summa -m 8192 -n 8192 -k 8192 -b 8192 -s a -v -p
